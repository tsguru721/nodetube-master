nav.center-block.text-center(aria-label='Page navigation example')
    ul.pagination
        li.page-item
            a.page-link(href=`/media/${recentAction}/${previousNumber}`) <

        each number in numbersArray
            li.page-item(class=(number == highlightedNumber) ? 'active' : undefined)
                a.page-link(href=`/media/${recentAction}/${number}`) #{number}

        li.page-item
            a.page-link(href=`/media/${recentAction}/${nextNumber}`) >

br